{
    "_version": "1.20.0",
    "sap.app": {
        "id": "sap.hr.hire",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "1.0.0"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "deviceTypes": {
            "phone": true,
            "tablet": true,
            "desktop": true
        },
        "supportedThemes": [
            "sap_belize"
        ]
    },
    "sap.ui5": {
        "contentDensities": {
            "cozy": true,
            "compact": true
        },
        "flexEnabled": true,
        "dependencies": {
            "libs": {
                "sap.m": {},
                "sap.f": {}
            },
            "minUI5Version": "1.60.0"
        },
        "models": {
            "i18n": {
                "uri": "i18n/i18n.properties",
                "type": "sap.ui.model.resource.ResourceModel"
            },
            "": {
                "uri": "model/data.json",
                "type": "sap.ui.model.json.JSONModel"
            }
        },
        "rootView": {
            "viewName": "sap.hr.hire.views.App",
            "type": "XML",
            "id": "idAppView"
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewPath": "sap.hr.hire.views",
                "viewType": "XML",
                "clearControlAggregation": true
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "home_page",
                    "target": [
                        "beginPage",
                        "emptyPage"
                    ]
                }
            ],
            "targets": {
                "beginPage": {
                    "viewName": "beginColumn",
                    "controlAggregation": "beginColumnPages",
                    "viewId": "beginPage"
                },
                "emptyPage": {
                    "viewId": "empty",
                    "viewName": "empty",
                    "controlAggregation": "midColumnPages"
                }
            }
        }
    }
}